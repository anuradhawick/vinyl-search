{
  "name": "vinyl-site",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "serve:ssr": "npm run dev:ssr && serverless offline start --httpPort 4200 --stage dev --noPrependStageInUrl --lambdaPort 4201",
    "dev:ssr": "ng build --output-hashing=none && ng run vinyl-site:server --bundleDependencies all && npm run compile:server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod --output-hashing=none && ng run vinyl-site:server:production --bundleDependencies all",
    "build:deploy:prod": "npm run build:ssr && serverless deploy --stage prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "~7.2.0",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@aws-amplify/auth": "^3.2.14",
    "@aws-amplify/storage": "^3.2.7",
    "@ckeditor/ckeditor5-angular": "^1.1.2",
    "@ckeditor/ckeditor5-build-classic": "^12.4.0",
    "@ckeditor/ckeditor5-build-inline": "^12.4.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.2",
    "@nguniversal/common": "^10.0.1",
    "@nguniversal/express-engine": "^8.2.6",
    "@nguniversal/module-map-ngfactory-loader": "v8.2.6",
    "aws-serverless-express": "^3.3.8",
    "compression": "^1.7.4",
    "core-js": "^2.6.11",
    "cors": "^2.8.5",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "jquery-ui-dist": "^1.12.1",
    "lodash": "^4.17.15",
    "ngx-toastr": "^10.2.0",
    "rxjs": "^6.5.4",
    "screenfull": "^5.0.2",
    "tslib": "^1.10.0",
    "uuid": "^3.3.3",
    "watermarkjs": "^2.0.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.24",
